เนื้อเรื่อง: package main

import (
    "regexp"
    "fmt"
)

func main() {
    var re = regexp.MustCompile(`(?mi)a|using System;

using System.Text.RegularExpressions;

public class Example

{

    public static void Main()

    {

        string pattern = @"a|b";

        string input = @"<p>Hello %DISPLAY_NAME%,<\p>

<p>Follow this link to verify your email address.<\p>

<p><a href='%LINK%'>%LINK%<\a><\p>

<p>If you didn’t ask to verify this address, you can ignore this email.<\p>

<p>Thanks,<\p>

<p>Your %APP_NAME% team<\p>

localhost	Default	

famous-store-313715.firebaseapp.com	Default	

famous-store-313715.web.app	Default	

Nate158.com

https:\\famous-store-313715.firebaseapp.com\__\auth\action";

        RegexOptions options = RegexOptions.IgnoreCase | RegexOptions.Multiline | RegexOptions.IgnorePatternWhitespace | RegexOptions.CultureInvariant;

        

        foreach (Match m in Regex.Matches(input, pattern, options))

        {

            Console.WriteLine("'{0}' found at index {1}.", m.Value, m.Index);

        }

    }

}

b|}
}|`)
    var str = `a or b, <?xml version='1.0' encoding='UTF-8'?><rss xmlns:atom="http://www.w3.org/2005/Atom" xmlns:openSearch="http://a9.com/-/spec/opensearchrss/1.0/" xmlns:blogger="http://schemas.google.com/blogger/2008" xmlns:georss="http://www.georss.org/georss" xmlns:gd="http://schemas.google.com/g/2005" xmlns:thr="http://purl.org/syndication/thread/1.0" version="2.0"><channel><atom:id>tag:blogger.com,1999:blog-5990361588879225255</atom:id><lastBuildDate>Wed, 15 Dec 2021 22:59:43 +0000</lastBuildDate><category>emoji</category><category>CLDR</category><category>Unicode</category><category>ICU</category><category>AAC</category><category>beta</category><category>IUC</category><category>UTR #51</category><category>adopt-a-character</category><category>9.0</category><category>POD</category><category>UTS #51</category><category>Gold Sponsor</category><category>LDML</category><category>The Unicode Standard</category><category>alpha</category><category>conference</category><category>IVD</category><category>UTS #46</category><category>cover art</category><category>paperback</category><category>Arabic</category><category>Collation</category><category>Survey Tool</category><category>UTS #10</category><category>UTS #18</category><category>UTS #39</category><category>locales</category><category>10646</category><category>7.0</category><category>8.0</category><category>SEI</category><category>emoji 12.0</category><category>10.0</category><category>CLDR 26</category><category>CLDR 36</category><category>CLDR 37</category><category>CLDR 39</category><category>IDNA</category><category>Mayan</category><category>UAX #9</category><category>UTR #50</category><category>Unicode 12</category><category>Unicode 13.0</category><category>Unicode 14</category><category>bidi</category><category>board of directors</category><category>cldr 38</category><category>cldr 40</category><category>membership</category><category>regular expression</category><category>repertoire</category><category>securi